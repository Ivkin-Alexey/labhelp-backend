datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  login     String      @id
  password  String
}

model Equipment {
  id                 String @id
  serialNumber       String
  inventoryNumber    String
  name               String
  description        String
  brand              String
  model              String
  category           String
  filesUrl           String
  imgUrl             String
}

model OperatingEquipment {
  id            String   @id
  userId        String
  isLongUse     Boolean
  startDateTime DateTime @default(now())
}


