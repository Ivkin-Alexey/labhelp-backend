   datasource db {
     provider = "postgresql"
     url      = env("DATABASE_URL")
   }

   generator client {
     provider = "prisma-client-js"
   }

   model Users {
     login     String      @id
     password  String
   }

model Equipments {
  id                 String @id @unique
  serialNumber       String
  inventoryNumber    String
  name               String
  description        String
  brand              String
  model              String
  category           String
  filesUrl           String
  imgUrl             String
}

